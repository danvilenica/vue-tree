{
  "name": "@grapoza/vue-tree",
  "version": "5.0.0",
  "description": "Tree components for Vue 3",
  "author": "Gregg Rapoza <grapoza@gmail.com>",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/grapoza/vue-tree",
    "type": "git"
  },
  "bugs": "https://github.com/grapoza/vue-tree/issues",
  "keywords": [
    "vue",
    "vuejs",
    "tree",
    "treeview"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/grapoza-tree-lib.umd.js",
  "module": "dist/grapoza-tree-lib.es.js",
  "exports": {
    ".": {
      "import": "./dist/grapoza-tree-lib.es.js",
      "require": "./dist/grapoza-tree-lib.umd.js"
    },
    "./css": "./dist/style.css"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite --open",
    "docs": "cd docsrc && @powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./generateDocs.ps1",
    "docs-resources-serve": "http-server ./dist -d false -i false -a 127.0.0.1 -p 8082",
    "docs-site-serve": "http-server ./docsrc/output -d false -i false -a 127.0.0.1 -p 8081 -o /index.html",
    "docs-site": "npm-run-all --parallel docs-resources-serve docs-site-serve",
    "serve": "vite preview",
    "test": "vitest run --coverage"
  },
  "dependencies": {
    "vue": "^3.2.6"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.2.4",
    "@vue/test-utils": "^2.0.0-rc.17",
    "c8": "^7.11.0",
    "http-server": "13.0.1",
    "jsdom": "^19.0.0",
    "npm-run-all": "4.1.5",
    "vite": "^2.8.6",
    "vitest": "^0.6.0",
    "vue-router": "^4.0.13"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "packageManager": "yarn@3.2.0"
}
